openapi: 3.0.0
info:
  title: TogoStanza on MicrobeDB.jp
  description: TogoStanza specifications on MicrobeDB.jp
  version: 1.0.0
paths:
  /biosample_environmental_metadata_info:
    get:
      tags:
        - BioSample
      description: This stanza describes environmental information of the specified
        BioSample.
      operationId: biosample_environmental_metadata_info
      parameters:
        - name: biosample_id
          in: query
          description: NCBI BioSample identifier
          required: true
          schema:
            type: string
            default: SAMN02232026
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /biosample_metadata_info:
    get:
      tags:
        - BioSample
      description: This stanza describes basic information of the specified BioSample.
      operationId: biosample_metadata_info
      parameters:
        - name: biosample_id
          in: query
          description: NCBI BioSample identifier
          required: true
          schema:
            type: string
            default: SAMN01041170
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_attributes:
    get:
      tags:
        - Disease
      description: This stanza describes basic information of the specified pathogenic
        disease based on PDO.
      operationId: disease_attributes
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000193
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_biosample_list:
    get:
      tags:
        - Disease
      description: This stanza lists BioSample ID of microbiome sequencing samples which
        are annotated to the specified pathogenetic disease.
      operationId: disease_biosample_list
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000193
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_gene_list:
    get:
      tags:
        - Disease
      description: This stanza lists related genes of the specified pathogenetic disease.
      operationId: disease_gene_list
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000193
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_lineage_info:
    get:
      tags:
        - Disease
      description: This stanza represents PDO ID's hierarchical relationship information in
        the ontology OWL file.
      operationId: disease_lineage_info
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000193
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_strain_list:
    get:
      tags:
        - Disease
      description: This stanza lists related strains of the specified pathogenetic disease,
        and their isolation sources, pathogenecity.
      operationId: disease_strain_list
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000193
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_symptom_list:
    get:
      tags:
        - Disease
      description: This stanza lists related symptom information of the specified
        pathogenetic disease based on PDO and CSSO relationship.
      operationId: disease_symptom_list
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000220
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /disease_taxonomy_list:
    get:
      tags:
        - Disease
      description: This stanza lists related taxa of the specified pathogenetic disease.
      operationId: disease_taxonomy_list
      parameters:
        - name: pdo_id
          in: query
          description: PDO identifier
          required: true
          schema:
            type: string
            default: PDO_000193
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_attributes:
    get:
      tags:
        - Environment
      description: This stanza describes MEO ID related information (MEO ID, Title (name of
        environment), Synonyms, and Comment (descriptions of the MEO ID)).
      operationId: environment_attributes
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_biosample_list:
    get:
      tags:
        - Environment
      description: This stanza lists BioSample IDs of genomes which strains were obtained
        from the specified environment.
      operationId: environment_biosample_list
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_biosample_taxonomy_composition:
    get:
      tags:
        - Environment
      description: "[Deprecated] Displaying taxonomic composition from biosample metadata"
      operationId: environment_biosample_taxonomy_composition
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_environmental_ortholog:
    get:
      tags:
        - Environment
      description: "[Deprecated]"
      operationId: environment_environmental_ortholog
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_function:
    get:
      tags:
        - Environment
      description: This stanza represents a graph of top 100 average functional composition
        (KO (KEGG Orthology), KEGG Pathway, MGFO (MBGD Microbial Gene Function
        Ontology seeAlso:http://mbgd.genome.ad.jp/htbin/function_category.pl))
        of metagenomic sequencing samples obtained from SRA, which were
        annotated to the MEO ID. Functional composition were analyzed by MeGAP.
        Average abundances of each functional category were calculated by sum of
        abundance of the functional category among all samples divided by total
        number of samples, which annotated to the MEO ID. Numbers of hit were
        normalized by length of CDS and number of assigned CDSs. Graph patterns
        can be choose from Bar chart, Treemap, and data for Functree. Data for
        Functree is a text data that can be used to the Functree web application
        (http://bioviz.tokyo/functree/) input.
      operationId: environment_function
      parameters:
        - name: meo_id
          in: query
          description: MEO ID
          required: true
          schema:
            type: string
            default: MEO_0000029
        - name: ortholog_type
          in: query
          description: Ortholog type(ko, pathway or mgfo). default value is pathway
          required: false
          schema:
            type: string
            default: pathway
        - name: chart
          in: query
          description: Visualization type(bar, treemap, table, functree). default value is
            bar
          required: false
          schema:
            type: string
            default: bar
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_function_table:
    get:
      tags:
        - Environment
      description: This stanza represents average functional composition (KO (KEGG
        Orthology), KEGG Pathway, MGFO (MBGD Microbial Gene Function Ontology
        seeAlso:http://mbgd.genome.ad.jp/htbin/function_category.pl), MBGD
        Ortholog) of metagenomic sequencing samples obtained from SRA, which
        were annotated to the MEO ID. Functional composition were analyzed by
        MeGAP.  Average abundances of each functional category were calculated
        by sum of abundance of the functional category among all samples divided
        by total number of samples, which annotated to the MEO ID. Numbers of
        hit were normalized by length of CDS and number of assigned CDSs.
      operationId: environment_function_table
      parameters:
        - name: meo_id
          in: query
          description: Environment ID
          required: true
          schema:
            type: string
            default: MEO_0000004
        - name: ortholog_type
          in: query
          description: ko, pathway or mbgd. default value is pathway
          required: false
          schema:
            type: string
            default: ko
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_gold_list:
    get:
      tags:
        - Environment
      description: "[Deprecated] Displaying strain genome list sampled from input
        environment"
      operationId: environment_gold_list
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_gold_table:
    get:
      tags:
        - Environment
      description: "[Deprecated] This stanza shows microbes genome list that was sequenced
        and mapped to ontology(MEO). Input a MEO ID ."
      operationId: environment_gold_table
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_linage_info:
    get:
      tags:
        - Environment
      description: This stanza represents MEO ID's hierarchical relationship information in
        the ontology OWL file.
      operationId: environment_linage_info
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_sample_amount_stackcharts:
    get:
      tags:
        - Environment
      description: This stanza summarizes accumulation of microbiome samples which sampled
        from an environment in the public sequence database (INSDC DRA/ERA/SRA).
        The environments of samples are categorized by MEO. The samples assigned
        to multiple MEO are multiply-counted in these MEO.
      operationId: environment_sample_amount_stackcharts
      parameters:
        - name: mode
          in: query
          description: sum or each
          required: false
          schema:
            type: string
            default: sum
        - name: meo_id
          in: query
          description: MEO identifier
          required: false
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_selectable_list:
    get:
      tags:
        - Other
      description: ""
      operationId: environment_selectable_list
      parameters:
        - name: referer
          in: query
          description: link uri of MicrobeDB
          required: false
          schema:
            type: string
            default: https://microbedb.jp
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_strain_list:
    get:
      tags:
        - Environment
      description: This stanza lists JCM or NBRC strain IDs which strains were isolated
        from the specified environment.
      operationId: environment_strain_list
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_taxonomy_composition_of_meta16s:
    get:
      tags:
        - Environment
      description: This stanza represents average taxonomic composition (Genus, Family,
        Order, Class, Phylum, Domain) of 16S rRNA gene amplicon sequencing
        samples obtained from SRA, which were annotated the MEO ID. Taxonomic
        compositions were analyzed by MeGAP. Ribosomal Database Project's taxa
        hierarchy were used. Since Family, Order, and Class are sometimes not
        defined in some taxa in RDP, interpretations of these taxonomic
        composition data should be noted. Average abundances of each taxon were
        calculated by sum of abundance of the taxon among all samples divided by
        total number of samples, which annotated to the MEO ID. Gray colored
        category in each pie chart indicates other assigned taxa.
      operationId: environment_taxonomy_composition_of_meta16s
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_taxonomy_composition_of_metagenome:
    get:
      tags:
        - Environment
      description: This stanza represents average taxonomic composition (Genus, Family,
        Order, Class, Phylum, Domain) of metagenomic sequencing samples obtained
        from SRA, which were annotated the MEO ID. Taxonomic composition were
        analyzed by MeGAP. Ribosomal Database Project's taxa hierarchy were
        used. Since Family, Order, and Class are sometimes not defined in some
        taxa in RDP, interpretations of these taxonomic composition data should
        be noted. Average abundances of each taxon were calculated by sum of
        abundance of the taxon among all samples divided by total number of
        samples, which annotated to the MEO ID. Gray colored category in each
        pie chart indicates other assigned taxa.
      operationId: environment_taxonomy_composition_of_metagenome
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_to_brother_function_comparison_stackbar:
    get:
      tags:
        - Environment
      description: "This stanza represents a graph of average functional composition (KEGG
        Pathway or MGFO) of samples which annotated to the specified MEO ID or
        hierarchically sister MEO IDs. "
      operationId: environment_to_brother_function_comparison_stackbar
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
        - name: ortholog_type
          in: query
          description: ko, pathway or mbgd. default value is pathway
          required: false
          schema:
            type: string
            default: pathway
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environment_to_brother_taxonomy_comparison_stackbar:
    get:
      tags:
        - Environment
      description: "This stanza represents a graph of average taxonomic composition
        (Domain, Phylum, Class, Order, Family, Genus) of samples which annotated
        to the specified MEO ID or hierarchically sister MEO IDs. "
      operationId: environment_to_brother_taxonomy_comparison_stackbar
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000029
        - name: type
          in: query
          description: domain, phylum, class, order, family or genus
          required: false
          schema:
            type: string
            default: phylum
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environments_function_comparison_stackbar:
    get:
      tags:
        - Multi Environment
      description: This stanza shows stackbar of function composition of input MEO IDs
      operationId: environments_function_comparison_stackbar
      parameters:
        - name: meo_id
          in: query
          description: MEO identifiers
          required: true
          schema:
            type: string
            default: MEO_0000029,MEO_0000030,MEO_0000024
        - name: ortholog_type
          in: query
          description: ko, pathway or mbgd
          required: false
          schema:
            type: string
            default: pathway
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /environments_taxonomy_comparison_stackbar:
    get:
      tags:
        - Multi Environment
      description: This stanza shows stackbar of microbial composition of input MEO IDs
      operationId: environments_taxonomy_comparison_stackbar
      parameters:
        - name: meo_id
          in: query
          description: "Input multi MEO ID by comma separated style "
          required: true
          schema:
            type: string
            default: MEO_0000029,MEO_0000030,MEO_0000024
        - name: type
          in: query
          description: Select one taxonomy rank (domain, phylum, class, order, family,
            genus)
          required: true
          schema:
            type: string
            default: phylum
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gene_annotation_list:
    get:
      tags:
        - Gene
      description: This stanza lists genes which matched to the user searched keyword in
        their annotation.
      operationId: gene_annotation_list
      parameters:
        - name: gene_name
          in: query
          description: Gene name
          required: true
          schema:
            type: string
            default: psbA
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gene_list_by_text:
    get:
      tags:
        - Search
      description: "This stanza lists genes of RefSeq genomes which annotation were matched
        to the user searched keyword. "
      operationId: gene_list_by_text
      parameters:
        - name: text
          in: query
          description: Search word
          required: true
          schema:
            type: string
            default: psbA
        - name: text_type
          in: query
          description: Searched object type
          required: false
          schema:
            type: string
            default: gene
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gene_ortholog_list:
    get:
      tags:
        - Gene
      description: This stanza lists MBGD ortholog cluster members of the specified cluster
        ID.
      operationId: gene_ortholog_list
      parameters:
        - name: gene_name
          in: query
          description: Gene name
          required: true
          schema:
            type: string
            default: psbA
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /genome_information_high_quality:
    get:
      tags:
        - Taxonomy
      description: This stanza lists high quality genomes from the specified taxon. The
        categorization of genomes to high quality or low quality was obtained
        from NCBI RefSeq. The category "high quality" indicates reference
        genomes or representative genomes of the taxon in NCBI RefSeq. If
        species name is specified, the summarized information of representative
        genome will be listed.
      operationId: genome_information_high_quality
      parameters:
        - name: tax_id
          in: query
          description: NCBI taxonomy ID
          required: true
          schema:
            type: string
            default: "562"
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /genome_information_low_quality:
    get:
      tags:
        - Taxonomy
      description: This stanza lists low quality genomes from the specified taxon. The
        categorization of genomes to high quality or low quality was obtained
        from NCBI RefSeq. The category "low quality" indicates genomes which are
        not categorized in reference genomes or representative genomes of the
        taxon in NCBI RefSeq. If species name is specified, the summarized
        information of representative genome will be listed.
      operationId: genome_information_low_quality
      parameters:
        - name: tax_id
          in: query
          description: NCBI taxonomy ID
          required: true
          schema:
            type: string
            default: "562"
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gold_taxonomy_compositon:
    get:
      tags:
        - Environment
      description: "[Deprecated] Displaying taxonomic composition from GOLD database
        annotation analysis"
      operationId: gold_taxonomy_compositon
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: true
          schema:
            type: string
            default: MEO_0000004
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_definition_info:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying gtps definition"
      operationId: gtps_definition_info
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_environment_info:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying input gtps annotated environment from GOLD
        database"
      operationId: gtps_environment_info
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_feature_info:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying feature information of input gtps"
      operationId: gtps_feature_info
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_genome_info:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying genome information of input gtps"
      operationId: gtps_genome_info
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_link_list:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying gtps associated resources link"
      operationId: gtps_link_list
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_ortholog_list:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying same MBGD ortholog cluster gene list of input
        gtps"
      operationId: gtps_ortholog_list
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /gtps_taxonomy_tree:
    get:
      tags:
        - GTPS
      description: "[Deprecated] Displaying gtps taxonomy information"
      operationId: gtps_taxonomy_tree
      parameters:
        - name: gtps_id
          in: query
          description: GTPS identifier
          required: true
          schema:
            type: string
            default: Plav_DS1:ST2899
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /kegg_attribute:
    get:
      tags:
        - Kegg
      description: This stanza describes basic information of specified KEGG pathway.
      operationId: kegg_attribute
      parameters:
        - name: kegg_id
          in: query
          description: Kegg KO identifier
          required: true
          schema:
            type: string
            default: K00010
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /latitude_to_longitude_sample_map:
    get:
      tags:
        - Statistics
      description: This stanza shows map and near saples against pin.
      operationId: latitude_to_longitude_sample_map
      parameters:
        - name: latitude_query
          in: query
          description: -90~90
          required: false
          schema:
            type: string
            default: "41.50857729743935"
        - name: longitude_query
          in: query
          description: -180~180
          required: false
          schema:
            type: string
            default: "-108.45603125"
        - name: distance_query
          in: query
          description: Input searching radius, unit is km
          required: false
          schema:
            type: string
            default: "3000"
      responses:
        "200":
          description: Successful Operation
        "400":
          description: Bad Request.
  /meo_ontology_viewer:
    get:
      tags:
        - Environment
      description: This stanza represents the hierarchical structure of MEO.
      operationId: meo_ontology_viewer
      parameters:
        - name: meo_id
          in: query
          description: MEO identifier
          required: false
          schema:
            type: string
            default: MEO_0000029
      responses:
        "200":
          description: Successful Operation
        "400":
